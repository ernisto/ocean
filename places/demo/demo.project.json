			"packages": {
				"$path": "../../roblox_packages",
				"ocean": {
					"$path": "../../src",
					"assets": {
						"$path": "../../assets",
						"$ignoreUnknownInstances": true
					}
				}
			},
			"client": {
				"$path": "client"
			}
		}
	}

{
	"name": "ocean",
	"emitLegacyScripts": false,
	"globIgnorePaths": [
		"**/*.spec.lua",
		"**/*.spec.luau",
		"**/*.bench.lua",
		"**/*.bench.luau",
		"**/*.story.lua",
		"**/*.story.luau"
	],
	"tree": {
		"$className": "DataModel",
		"Lighting": {
			"$path": "../../places/lighting.project.json"
		},
		"TextChatService": {
			"$path": "../../places/chat.project.json"
		},
		"Players": {
			"$path": "../../places/players.project.json"
		},
